{
    "env": { 
        "browser": true,
        "es2021": true 
    },
    "extends": [ 
        "eslint:recommended",
        "prettier"
    ],
    "parser": "babel-eslint", 
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11, 
        "sourceType": "module" 
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": 0 
    }
}